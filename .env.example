OPENAI_API_KEY=
DATABASE_URL=postgresql://user:password@localhost:5432/mydatabase # AI가 접근할 DB 접속 정보
TARGET_PROJECT_ROOT=/Users/username/my-nextjs-project  # AI가 접근할 대상 프로젝트 경로

# 요청 받는 서버 (이 FastAPI 서버) 주소·포트
SERVER_HOST=localhost
SERVER_PORT=8000

# 요청 하는 클라이언트(예: Next.js) 출처 (CORS 허용)
CLIENT_ORIGIN=http://localhost:3000